{
  "name": "eslint-prettier-config",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "prettier-eslint \"src/**/*.js\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oOBobbyOo/eslint-prettier-config.git"
  },
  "keywords": [
    "eslint",
    "prettier"
  ],
  "author": "Bobby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oOBobbyOo/eslint-prettier-config/issues"
  },
  "homepage": "https://github.com/oOBobbyOo/eslint-prettier-config#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier-eslint": "^8.8.2"
  }
}
